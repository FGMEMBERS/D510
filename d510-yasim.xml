<?xml version="1.0" encoding="UTF-8"?>

<!-- Dewoitine D.510 - 2010 Emmanuel BARANGER
   
     Dewoitine

     sources          : http://en.wikipedia.org/wiki/Dewoitine_D.500

     wingspan         : 12.09 m    (   39 ft 8 in)
     length           :  7.94 m    (   26 ft 1 in)
     height           :  2.42 m    (    7 ft 11 in)
     max speed        :   402 km/h (  217 kt )
     empty weight     :  1496 kg   ( 3298 lb)
     engine           : Hispano-Suiza 12Ycrs V12 engine, ( 860 hp at 16405 ft)
-->

<airplane mass="3298" >

  <approach speed="80" aoa="4" >
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.2"/>
    <control-setting axis="/controls/engines/engine[0]/mixture" value="1.0"/>
    <solve-weight idx="0" weight="180"/>
    <solve-weight idx="1" weight="0"/>
  </approach>
  
  <cruise speed="195" alt="5000" >
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.8"/>
    <control-setting axis="/controls/engines/engine[0]/mixture" value="0.8" />
    <solve-weight idx="0" weight="180"/>
    <solve-weight idx="1" weight="0"/>
  </cruise>

  <cockpit x="0.593" y="0" z="0.870"/>

  <fuselage ax="3.537" ay="0.0" az="0.302" bx="-3.190" by="0" bz="0.230" width="0.931" taper="0.45" midpoint="0.5"/>

  <wing x="1.289" y="0.470" z="-0.195" 
        chord="1.825" 
        length="5.552" 
        taper="0.70" 
	dihedral="2" 
        sweep="-0.7"
	camber="0.01">
        <stall aoa="16" width="5" peak="1.5"/>
        <flap0 start="0.2" end="0.90" lift="1.4" drag="1.1"/>

        <control-input axis="/controls/flight/aileron" control="FLAP0" split="true"/>
        <control-input axis="/controls/flight/aileron-trim" control="FLAP0" split="true"/>

        <control-output control="FLAP0" side="left" prop="surface-positions/left-aileron-pos-norm"/>
        <control-output control="FLAP0" side="right" prop="surface-positions/right-aileron-pos-norm"/>
  </wing>
  
  <hstab x="-2.910" y="0" z="0.612" 
	 chord="1.110" 
	 length="1.788" 
         taper="0.38"
         sweep="3"> 
         <stall aoa="16" width="8" peak="1.5"/>
         <flap0 start="0.0" end="1.0" lift="1.55" drag="1.3"/>

         <control-input axis="/controls/flight/elevator" control="FLAP0"/>
         <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>

         <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
  </hstab>

  <vstab x="-2.678" y="0" z="-0.039" 
	 chord="2.319" 
	 length="1.977" 
         taper="0.26" 
	 sweep="20">
         <stall aoa="14" width="4" peak="1.5"/>
         <flap0 start="0" end="1" lift="1.3" drag="1.3"/>

         <control-input axis="/controls/flight/rudder" square="true" control="FLAP0" invert="true"/>
         <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>

         <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"  min="1" max="-1"/>
  </vstab>

  <!-- Engines   source            : http://en.wikipedia.org/wiki/Hispano-Suiza_12Y

                 name              : Hispano-Suiza 12Y
                 type              : Twelve-cylinder supercharged liquid-cooled 60Â° V12 engine
                 power cruise      : 860 hp at 2400 rpm at 16405 ft
                 weight            :    465 kg  (1025 lb)
                 displacement      : 2199.9 in3 (36.05 l)
                 compression       : 5.8:1
  -->
  <propeller x="2.605" y="0.0" z="0.327"
             radius="1.45"
             mass="1025"
             moment="35"
             cruise-speed="195"
             cruise-alt="5000"
             cruise-power="780"
             cruise-rpm="1400"
             takeoff-power="860"
             takeoff-rpm="2400"
             gear-ratio="0.5">
             <piston-engine eng-rpm="2400"
                            eng-power="860"
                            displacement="1751"
                            compression="5.8"/>
             <actionpt x="3.633" y="0.0" z="0.327"/>
             <control-input control="THROTTLE" axis="/controls/engines/engine[0]/throttle"/>
             <control-input control="STARTER" axis="/controls/engines/engine[0]/starter"/>
             <control-input control="MAGNETOS" axis="/controls/engines/engine[0]/magnetos"/>
             <control-input control="MIXTURE" axis="/controls/engines/engine[0]/mixture"/>
  </propeller>

  <!-- left main -->
  <gear x="2.050" y="1.684" z="-1.774" 
        compression="0.4"
        retract-time="5"
        spring="0.8"
        damp="1.2"
        sfric="1.0"
        dfric="1.0">
        <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="5.1"/>

        <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>
  </gear>

  <!-- right main -->
  <gear x="2.050" y="-1.684" z="-1.774"
        compression="0.4"
        retract-time="5"
        spring="0.8"
        damp="1.2"
        sfric="1.0"
        dfric="1.0">
        <control-input axis="/controls/gear/brake-right" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="5.1"/>

        <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>
  </gear>

  <!-- Tail wheel; has castering selectable by a wheel lock -->
  <gear x="-3.218" y="0" z="-0.227"
        compression="0.2"
        spring="0.8"
        damp="1.2"
        sfric="1.0"
        dfric="1.0">
  </gear>

  <!-- Fuel  -->  
  <tank x="1.5" y="1.7" z="-0.27" capacity="210" />
  <tank x="-1.25" y="0" z="-0.33" capacity="156" />
  <tank x="1.5" y="-1.7" z="-0.27" capacity="210" />

  <weight x="0.75" y="0" z="-0.5" mass-prop="/sim/weight[0]/weight-lb"/>
  <weight x="-1.0" y="0" z="-0.5" mass-prop="/sim/weight[1]/weight-lb"/>

  <!-- CG theorique centre de la corde x=1.140 y=0.0 z=-0.400
  <ballast x="4.23" y="0.001" z="-3.16" mass="600" /> -->

</airplane>
