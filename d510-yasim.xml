<airplane mass="3298" >

  <approach speed="80" aoa="4" >
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.05"/>
    <control-setting axis="/controls/engines/engine[0]/mixture" value="1.0"/>
    <solve-weight idx="0" weight="180"/>
    <solve-weight idx="1" weight="0"/>
  </approach>
  
  <cruise speed="195" alt="5000" >
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.8"/>
    <control-setting axis="/controls/engines/engine[0]/mixture" value="0.8" />
    <solve-weight idx="0" weight="180"/>
    <solve-weight idx="1" weight="0"/>
  </cruise>

  <cockpit x="0.744" y="0" z="0.870"/>

  <fuselage ax="3.537" ay="0.0" az="0.302" bx="-3.190" by="0" bz="0.230" width="1.101" taper="0.55" midpoint="0.357" idrag="1.75"/>
  <fuselage ax="2.805" ay="0.0" az="-0.507" bx="1.644" by="0.0" bz="-0.507" width="0.599" taper="0.59" midpoint="0.52" idrag="1.6"/>
  <fuselage ax="2.480" ay="1.686" az="-1.324" bx="1.540" by="1.686" bz="-1.324" width="0.593" taper="0.604" midpoint="0.475" idrag="1.2"/>
  <fuselage ax="2.480" ay="-1.686" az="-1.324" bx="1.540" by="-1.686" bz="-1.324" width="0.593" taper="0.604" midpoint="0.475" idrag="1.2"/>
  <fuselage ax="1.678" ay="1.981" az="-0.342" bx="1.201" by="1.981" bz="-0.342" width="0.206" taper="0.777" midpoint="0.3563" idrag="1.05"/>
  <fuselage ax="1.678" ay="-1.981" az="-0.342" bx="1.201" by="-1.981" bz="-0.342" width="0.206" taper="0.777" midpoint="0.3563" idrag="1.05"/>
  <fuselage ax="0.944" ay="0" az="0.799" bx="0.810" by="0" bz="0.799" width="0.430" taper="0.3139" midpoint="0.694" idrag="1.37"/>

  <wing x="1.289" y="0.470" z="-0.195" 
        chord="1.825" 
        length="5.552" 
        taper="0.60" 
        dihedral="2" 
        sweep="-0.7"
        camber="0.01">
        <stall aoa="16" width="5" peak="1.5"/>
        <flap0 start="0.2" end="0.90" lift="1.2" drag="1.25"/>

        <control-input axis="/controls/flight/aileron" control="FLAP0" split="true"/>
        <control-input axis="/controls/flight/aileron-trim" control="FLAP0" split="true"/>

        <control-output control="FLAP0" side="left" prop="surface-positions/left-aileron-pos-norm"/>
        <control-output control="FLAP0" side="right" prop="surface-positions/right-aileron-pos-norm"/>
  </wing>
  
  <hstab x="-2.910" y="0" z="0.612" 
	 chord="1.110" 
	 length="1.788" 
         taper="0.38"
         sweep="3"> 
         <stall aoa="16" width="8" peak="1.5"/>
         <flap0 start="0.0" end="1.0" lift="1.55" drag="1.3"/>

         <control-input axis="/controls/flight/elevator" control="FLAP0"/>
         <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>

         <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
  </hstab>


  <vstab x="-2.678" y="0" z="0.059" 
         chord="2.319" 
         length="1.807" 
         taper="0.26" 
         sweep="22">
         <stall aoa="14" width="4" peak="1.5"/>
         <flap0 start="0" end="1" lift="1.65" drag="1.15"/>

         <control-input axis="/controls/flight/rudder" square="true" control="FLAP0" invert="true"/>
         <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>

         <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"  min="1" max="-1"/>
  </vstab>

  <!-- 
       Les elements vstab definis ici correspondent aux jambes de train 
       qui influent sur l'aerodynamique de l'avion, et en particulier sur la trainee.
  -->
  <vstab x="1.608" y="-1.659" z="-0.213"
         chord="0.162"
         length="1.242"
         taper="1.0"
         sweep="-22"
         dihedral="-90">
         <stall aoa="15" width="10" peak="1"/>
         <spoiler start="0" end="1" lift="0" drag="20.5" idrag="1.5"/>

         <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
  </vstab>
  <vstab x="1.608" y="1.659" z="-0.213"
         chord="0.162"
         length="1.242"
         taper="1.0"
         sweep="-22"
         dihedral="-90">
         <stall aoa="15" width="10" peak="1"/>
         <spoiler start="0" end="1" lift="0" drag="20.5" idrag="1.5"/>

         <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
  </vstab>
  <vstab x="1.664" y="-0.413" z="-0.308"
        chord="0.065"
        length="1.684"
        taper="1.9"
        sweep="-15"
        dihedral="-135">
        <stall aoa="15" width="15" peak="1"/>
        <spoiler start="0" end="1" lift="0" drag="15.0" idrag="1.05"/>

        <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
  </vstab>
  <vstab x="1.664" y="0.413" z="-0.308"
         chord="0.065"
         length="1.684"
         taper="1.9"
         sweep="-15"
         dihedral="-45">
         <stall aoa="15" width="15" peak="1"/>
         <spoiler start="0" end="1" lift="0" drag="15.0" idrag="1.05"/>

         <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
  </vstab>
  <vstab x="2.704" y="0.336" z="-0.184"
         chord="0.087"
         length="1.898"
         taper="1.0"
         sweep="22"
         dihedral="-45">
         <stall aoa="15" width="15" peak="1"/>
         <spoiler start="0" end="1" lift="0" drag="15.0" idrag="1.05"/>

         <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
  </vstab>
  <vstab x="2.704" y="-0.336" z="-0.184"
         chord="0.087"
         length="1.898"
         taper="1.0"
         sweep="22"
         dihedral="-135">
         <stall aoa="15" width="15" peak="1"/>
         <spoiler start="0" end="1" lift="0" drag="15.0" idrag="1.05"/>

         <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
  </vstab>

  <!-- Engines   source            : http://en.wikipedia.org/wiki/Hispano-Suiza_12Y

                 name              : Hispano-Suiza 12Y
                 type              : Twelve-cylinder supercharged liquid-cooled 60° V12 engine
                 power cruise      : 860 hp at 2400 rpm at 16405 ft
                 weight            :    465 kg  (1025 lb)
                 displacement      : 2199.9 in3 (36.05 l)
                 compression       : 5.8:1
  -->
  <propeller x="2.605" y="0.0" z="0.327"
             radius="1.55"
             mass="1025"
             moment="15"
             cruise-speed="195"
             cruise-alt="5000"
             cruise-power="600"
             cruise-rpm="1400"
             takeoff-power="690"
             takeoff-rpm="2400"
             gear-ratio="0.59">
             <piston-engine eng-rpm="2600"
                            eng-power="860"
                            displacement="2199"
                            compression="5.8"/>
             <actionpt x="5.633" y="0.0" z="0.327"/>
             <control-input control="THROTTLE" axis="/controls/engines/engine[0]/throttle"/>
             <control-input control="STARTER" axis="/controls/engines/engine[0]/starter"/>
             <control-input control="MAGNETOS" axis="/controls/engines/engine[0]/magnetos"/>
             <control-input control="MIXTURE" axis="/controls/engines/engine[0]/mixture"/>
  </propeller>

  <!-- left main -->
  <gear x="2.050" y="1.684" z="-1.774" 
        compression="0.25"
        retract-time="5"
        spring="0.35"
        damp="1.5"
        sfric="1.95"
        dfric="1.25"
        initial-load="0.35">
        <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="1.1"/>

        <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>
  </gear>

  <!-- right main -->
  <gear x="2.050" y="-1.684" z="-1.774"
        compression="0.25"
        retract-time="5"
        spring="0.35"
        damp="1.5"
        sfric="1.95"
        dfric="1.25"
        initial-load="0.35">
        <control-input axis="/controls/gear/brake-right" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-speed control="EXTEND" transition-time="1.1"/>

        <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>
  </gear>

  <!-- Tail wheel; has castering selectable by a wheel lock -->
  <gear x="-3.218" y="0" z="-0.227"
        compression="0.2"
        spring="1.8"
        damp="0.2"
        sfric="0.95"
        dfric="0.5">
  </gear>

  <!-- Fuel  -->  
  <tank x="1.5" y="1.7" z="-0.27" capacity="217" />
  <tank x="-1.25" y="0" z="-0.33" capacity="260" />
  <tank x="1.5" y="-1.7" z="-0.27" capacity="217" />

  <weight x="0.75" y="0" z="-0.5" mass-prop="/sim/weight[0]/weight-lb"/>
  <weight x="-1.0" y="0" z="-0.5" mass-prop="/sim/weight[1]/weight-lb"/>

  <!-- CG theorique centre de la corde x=1.140 y=0.0 z=-0.400
  <ballast x="4.23" y="0.001" z="-3.16" mass="600" /> -->

</airplane>

