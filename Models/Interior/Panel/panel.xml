<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ASI -->
  <model>
    <path>Aircraft/D510/Models/Interior/Panel/Instruments/airspeed/airspeed.xml</path>
    <offsets>
      <x-m> 0.015 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.020 </z-m>
    </offsets>
  </model>

  <!-- ALT -->
  <model>
    <path>Aircraft/D510/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m> 0.015 </x-m>
      <y-m> 0.070 </y-m>
      <z-m> 0.000 </z-m>
    </offsets>
  </model>

  <!-- VSI -->
  <model>
    <path>Aircraft/D510/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.015 </x-m>
      <y-m> -0.080 </y-m>
      <z-m>  0.010 </z-m>
    </offsets>
  </model>

  <!-- CLOCK -->
  <model>
    <path>Aircraft/D510/Models/Interior/Panel/Instruments/clock/clock.xml</path>
    <offsets>
      <x-m>  0.015 </x-m>
      <y-m> -0.140 </y-m>
      <z-m> -0.010 </z-m>
    </offsets>
  </model>

  <!-- COMPASS -->
  <model>
    <path>Aircraft/D510/Models/Interior/Panel/Instruments/compass/compass.xml</path>
    <offsets>
      <x-m>  0.020 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.040 </z-m>
    </offsets>
  </model>

</PropertyList>
