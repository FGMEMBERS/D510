<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>support</object-name>
    <object-name>sousthrottleL</object-name>
    <object-name>sousthrottleR</object-name>
    <object-name>throttleL</object-name>
    <object-name>throttleR</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
   <type>rotate</type>
   <object-name>throttleL</object-name>
   <property>/controls/engines/engine[0]/throttle</property>
   <interpolation>
    <entry>
     <ind>0.0</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>110</dep>
    </entry>
   </interpolation>
   <center>
     <x-m> -0.793 </x-m>
     <y-m> -0.024 </y-m>
     <z-m> 0.147 </z-m>
   </center>
   <axis>
     <x> 0 </x>
     <y>-1 </y>
     <z> 0 </z>
   </axis>
  </animation>
 
  <animation>
    <type>rotate</type>
    <object-name>throttleR</object-name>
    <property>/controls/engines/engine/mixture/</property>
    <interpolation>
     <entry>
      <ind>0.0</ind>
      <dep>0</dep>
     </entry>
     <entry>
      <ind>1.0</ind>
      <dep>90</dep>
     </entry>
    </interpolation>
    <center>
     <x-m> -0.793 </x-m>
     <y-m>  0.018 </y-m>
     <z-m>  0.147 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
