<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>divers.ac</path>

  <animation>
    <object-name>divers</object-name>
    <object-name>molette</object-name>
    <object-name>manette</object-name>
    
  </animation>

  <!-- SWITCH -->
  <model>
    <path>Aircraft/D510/Models/Interior/Panel/Instruments/switch/switch.xml</path>
    <offsets>
      <x-m>  0.005 </x-m>
      <y-m>  0.013 </y-m>
      <z-m> -0.041 </z-m>
    </offsets>
  </model>

  <!-- RADIO COMPASS -->
  <model>
    <path>Aircraft/D510/Models/Interior/Panel/Instruments/radio-compass/radio-compass.xml</path>
    <offsets>
      <x-m>  0.00228 </x-m>
      <y-m> -0.01600 </y-m>
      <z-m> -0.01300 </z-m>
    </offsets>
  </model>

</PropertyList>
